            if ($(".txtMessage").val() != "") {
                var conf = {
                    title: "룰렛 이벤트",
                    typeAnimated: true,
                    boxWidth: '80%',
                    useBootstrap: false,
                    content: $(".txtMessage").val(),
                    buttons: {
                        "확인": {
                            btnClass: 'btn-blue',
                            action: function () {
                                
                            }
                        }
                    }
                };
                $.confirm(conf);
            }
        });
    </script>
